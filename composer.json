{
    "name": "mpsoft/mpmassimport",
    "description": "Importazione massiva prodotti",
    "authors": [
        {
            "name": "Massimiliano Palermo",
            "email": "maxx.palermo@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.4.0",
        "prestashop/prestashop-webservice-lib": "dev-master",
        "phpoffice/phpspreadsheet": "^1.29",
        "fawno/wget": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "MpSoft\\MpMassImport\\": "src/",
            "MpSoft\\MpMassImport\\WebServices\\": "vendor/prestashop/prestashop-webservice-lib/",
            "MpSoft\\MpMassImport\\Xlsx\\": "xlsx/",
            "MpSoft\\MpMassImport\\Plugins\\": "plugins/"
        },
        "classmap": [],
        "exclude-from-classmap": [],
        "files": []
    },
    "config": {
        "preferred-install": "dist",
        "prepend-autoloader": false,
        "sort-packages": true,
        "platform-check": "true",
        "platform": {
            "php": "7.4.0"
        }
    },
    "type": "prestashop-module"
}
